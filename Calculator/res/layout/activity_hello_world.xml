<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:longClickable="false"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:weightSum="1.0"
    android:background="@drawable/blue"
    tools:context="com.test.helloworld.HelloWorld">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button1"
        android:gravity="left"
        android:text="@string/hello_world"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView1"
        android:layout_marginTop="22dp"
        android:onClick="pressone"
        android:textStyle="bold"
        android:textSize="35dip"
        android:background="@drawable/button_click"
        android:text="1" />

    <Button
        android:id="@+id/button2"
        android:textStyle="bold"
        android:textSize="35dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button1"
        android:layout_alignBottom="@+id/button1"
        android:layout_toRightOf="@+id/button1"
        android:background="@drawable/button_click"
        android:onClick="presstwo"
        
        android:text="2" />

    <Button
        android:id="@+id/button4"
        android:textStyle="bold"
        android:textSize="35dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button1"
        android:layout_below="@+id/button1"
        android:layout_toLeftOf="@+id/button5"
        android:onClick="pressfour"
        android:background="@drawable/button_click"
        android:text="4" />

    <Button
        android:id="@+id/button5"
        android:textStyle="bold"
        android:textSize="35dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button4"
        android:layout_alignBottom="@+id/button4"
        android:layout_toLeftOf="@+id/button3"
        android:onClick="pressfive"
        android:background="@drawable/button_click"
        android:text="5" />

    <Button
        android:id="@+id/button6"
        android:textStyle="bold"
        android:textSize="35dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button5"
        android:layout_alignBottom="@+id/button5"
        android:layout_alignLeft="@+id/button3"
        android:background="@drawable/button_click"
        android:onClick="presssix"
        android:text="6" />

    <Button
        android:id="@+id/button8"
        android:textStyle="bold"
        android:textSize="35dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/button5"
        android:layout_below="@+id/button4"
        android:background="@drawable/button_click"
        android:onClick="presseight"
        android:text="8" />

    <Button
        android:id="@+id/button9"
        android:textStyle="bold"
        android:textSize="35dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button8"
        android:layout_alignBottom="@+id/button8"
        android:layout_alignRight="@+id/button6"
        android:background="@drawable/button_click"
        android:onClick="pressnine"
        android:text="9" />

    <Button
        android:id="@+id/button3"
        android:textStyle="bold"
        android:textSize="35dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button6"
        android:layout_toRightOf="@+id/button2"
        android:onClick="pressthree"
        android:background="@drawable/button_click"
        android:text="3" />

    <Button
        android:id="@+id/buttonplus"
        android:textStyle="bold"
        android:textSize="35dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button4"
        android:layout_marginTop="49dp"
        android:layout_toLeftOf="@+id/button8"
        android:onClick="pressplus"
        android:background="@drawable/button_click"
        android:text="+" />

    <Button
        android:id="@+id/buttonsubtract"
        android:layout_width="wrap_content"
        android:textStyle="bold"
        android:textSize="35dip"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/buttonplus"
        android:layout_toLeftOf="@+id/button9"
        android:onClick="presssubtract"
        android:background="@drawable/button_click"
        android:text="-" />

    <Button
        android:id="@+id/button7"
        android:textStyle="bold"
        android:textSize="35dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button8"
        android:layout_alignBottom="@+id/button8"
        android:layout_alignLeft="@+id/buttonplus"
        android:layout_alignRight="@+id/button4"
        android:onClick="press7"
        android:background="@drawable/button_click"
        android:text="7" />

    <Button
        android:id="@+id/buttontimes"
        android:textStyle="bold"
        android:textSize="35dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/buttonplus"
        android:layout_toRightOf="@+id/buttonplus"
        android:onClick="presstimes"
        android:background="@drawable/button_click"
        android:text="*" />

    <Button
        android:id="@+id/buttonover"
        android:textStyle="bold"
        android:textSize="35dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/buttonplus"
        android:layout_toLeftOf="@+id/buttonsubtract"
        android:onClick="pressover"
        android:background="@drawable/button_click"
        android:text="/" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button10"
        android:layout_alignLeft="@+id/button10"
        android:layout_alignRight="@+id/textView2"
        android:layout_below="@+id/textView2"
        android:textStyle="bold"
        android:text="History"
        android:textSize="15dip"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonover"
        android:layout_alignRight="@+id/buttontimes"
        android:layout_below="@+id/buttontimes"
        android:text="   "
        android:textSize="25dip" />

    <Button
        android:id="@+id/button10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignParentBottom="true"
        android:onClick="reset"
        android:background="@drawable/button_click"
        android:text="Clear" />

    <Button
        android:id="@+id/button11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button10"
        android:layout_alignBottom="@+id/button10"
        android:layout_alignLeft="@+id/buttontimes"
        android:background="@drawable/button_click"
        android:layout_marginLeft="19dp"
        android:onClick="clearhistory"
        android:text="clear the histroy" />

    <Button
        android:id="@+id/buttonauthor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button1"
        android:layout_alignLeft="@+id/button3"
        android:background="@android:color/transparent"
        android:onClick="showauthor"
        android:text="联系作者" />

    <Button
        android:id="@+id/buttondot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/buttonzero"
        android:layout_toRightOf="@+id/buttonsubtract"
        android:background="@drawable/button_click"
        android:text=" ."
        android:onClick="pressdot"
        android:textSize="35dip"
        android:textStyle="bold" />

    <Button
        android:id="@+id/buttonequal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView2"
        android:layout_toRightOf="@+id/textView3"
        android:background="@drawable/button_click"
        android:onClick="pressequal"
        android:text="="
        android:textSize="35dip"
        android:textStyle="bold" />

    <Button
        android:id="@+id/newbutton"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonzero"
        android:layout_alignRight="@+id/buttonequal"
        android:layout_below="@+id/buttonequal"
        android:background="@drawable/button_click"
        android:onClick="go"
        android:textStyle="normal"
        android:text="真源香谷" />

    <Button
        android:id="@+id/buttonzero"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button9"
        android:layout_below="@+id/button9"
        android:background="@drawable/button_click"
        android:onClick="presszero"
        android:text="0"
        android:textSize="35dip"
        android:textStyle="bold" />

    <Button
        android:id="@+id/newbutton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/newbutton"
        android:layout_below="@+id/newbutton"
        android:background="@drawable/button_click"
        android:text="打给爸爸"
        android:textSize="15dip"
        android:onClick="go2"
        android:textStyle="normal" />

</RelativeLayout>